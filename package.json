    "forever": "^0.15.1"
  },
  "devDependencies": {
    "mocha": "^2.4.1",
    "chai": "^3.4.1",
    "supertest": "^1.1.0",
    "bunyan-prettystream": "hadfieldn/node-bunyan-prettystream"
  },
  "engine": {
    "node": ">=0.10"
  }
